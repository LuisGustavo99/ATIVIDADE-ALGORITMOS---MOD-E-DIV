programa {
  funcao inicio() 
  {
     inteiro segundos, idade, idade_em_segundos, segundos_anos

     escreva("quantos anos você tem?")
     leia(idade)
  
     segundos_anos = 365 * 86400
     idade_em_segundos = segundos_anos * idade
     
     escreva("sua idade em segundo é " , idade_em_segundos)
     
     
  }
}
