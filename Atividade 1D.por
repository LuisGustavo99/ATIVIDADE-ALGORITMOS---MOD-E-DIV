programa {
  funcao inicio() 
  {
    inteiro chocolates, anos_vividos, chocolates_total

    escreva("Quantos chocolates você pretende comer em 1 anos")
    leia(chocolates)

    escreva("Quantos anos você vai viver?")
    leia(anos_vividos)

    chocolates_total = chocolates * anos_vividos
    escreva("Você vai comer " , chocolates_total , " na vida toda")
  }
}
